package p2;

import p1.EAM;

public class Fak_Rechner
{

	public static void main(String[] args)
	{
		System.out.println( Fak_Rechner.CalcFak(5));
	}
	public static int CalcFak (int p_number)
	{
		if(p_number > 1)
			return CalcFak(p_number * (p_number-1));
		else if(p_number == 0)
			return 0;
		else
			return 1;

			
			
	}	
	
}
